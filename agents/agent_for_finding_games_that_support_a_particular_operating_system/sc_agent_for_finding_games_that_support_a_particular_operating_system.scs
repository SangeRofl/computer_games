sc_agent_for_finding_games_that_support_a_particular_operating_system
=> nrel_main_idtf:
	[sc-агент поиска игр, поддерживающих определённую операционную систему] (* <- lang_ru;; *);
	[sc-agent of finding games that support a particular operating system] (* <- lang_en;; *);
	
<- abstract_sc_agent;

=> nrel_primary_initiation_condition: 
	(sc_event_add_output_arc => question_initiated);
	
=> nrel_initiation_condition_and_result: 
	(..sc_agent_for_finding_games_that_support_a_particular_operating_system_condition => ..sc_agent_for_finding_games_that_support_a_particular_operating_system_result);
	
<= nrel_sc_agent_key_sc_elements: 
	{
	question_initiated;
	question;
	question_finding_games_that_support_a_particular_operating_system
	};


=> nrel_inclusion: 
	.platform_independent_realization_of_sc_agent_for_finding_games_that_support_a_particular_operating_system
	(*
	<- platform_independent_abstract_sc_agent;;
	<= nrel_sc_agent_program: 
		{
		agent_for_finding_games_that_support_a_particular_operating_system		
		};;
		
	-> sc_agent_for_finding_games_that_support_a_particular_operating_system_scp (* <- active_sc_agent;; *);;
	*);;

..sc_agent_for_finding_games_that_support_a_particular_operating_system_condition
= [*
	question_finding_games_that_support_a_particular_operating_system _-> .._question;;
	question_initiated _-> .._question;;
	question _-> .._question;;
	.._question _-> rrel_1:: .._param1;;
	.._question _-> rrel_2:: .._param2;;
*];;
..sc_agent_for_finding_games_that_support_a_particular_operating_system_result
= [*
	question_finding_games_that_support_a_particular_operating_system _-> .._question;;
	question_finished _-> .._question;;
	question _-> .._question;;
	.._question _=> nrel_answer:: .._answer;;
	.._question _-> rrel_1:: .._param1;;
	.._question _-> rrel_2:: .._param2;;
*];;
