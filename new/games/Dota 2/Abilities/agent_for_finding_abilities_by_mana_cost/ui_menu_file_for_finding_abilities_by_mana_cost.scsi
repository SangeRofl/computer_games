ui_menu_file_for_finding_abilities_by_mana_cost
	<- 	ui_user_command_class_atom; 
		ui_user_command_class_view_kb;
	=> nrel_main_idtf: 
		[Найти способности по расходу маны](* <- lang_ru;; *);
		[Find abilities by mana cost] (* <- lang_en;; *);
	=> ui_nrel_command_template:
		[*
		question_for_finding_abilities_by_mana_cost _-> ._question_for_finding_abilities_by_mana_cost_instance
			(*
			_-> ui_arg_1;;
			*);;
		._question_for_finding_abilities_by_mana_cost_instance _<- question;;
		*];
	=> ui_nrel_command_lang_template: 
		[Запрос поиска способностей с расходом маны до $ui_arg_1] 
		(* <- lang_ru;; *);
		[Request of finding abilities with mana cost below $ui_arg_1] 
		(* <- lang_en;; *);;

