ui_menu_file_for_finding_available_subject_at_fix_price
	<- 	ui_user_command_class_atom; 
		ui_user_command_class_view_kb;
	=> nrel_main_idtf: 
		[Найти доступные предметы по фиксированной цене](* <- lang_ru;; *);
		[Find available subject at fix price] (* <- lang_en;; *);
	=> ui_nrel_command_template:
		[*
		question_for_finding_available_subject_at_fix_price _-> ._question_for_finding_available_subject_at_fix_price_instance
			(*
			_-> ui_arg_1;;
			*);;
		._question_for_finding_available_subject_at_fix_price_instance _<- question;;
		*];
	=> ui_nrel_command_lang_template: 
		[Запрос поиска доступных предметов с фиксированной ценой до $ui_arg_1] 
		(* <- lang_ru;; *);
		[Request of finding available subject at fix price below $ui_arg_1] 
		(* <- lang_en;; *);;

